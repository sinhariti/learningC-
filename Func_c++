#include<cstdio>
//CREATE FUNCTION 
void func(){
    puts("This is function.");
}
int main(){
    puts("This is main()");
    func();
    return 0;
}
//CREATING FUNCTIONS WITH PARAMETER 

#include<cstdio>
void func(int i){
    puts("This is function.");
    printf("This is func(%i)\n",i);
}
int main(){
    puts("This is main()");
    func(4);
    return 0;
}
